{
    // "idf.port": "/dev/ttyUSB0",
    "idf.adapterTargetName": "esp32",
    "idf.openOcdConfigs": [
        "${workspaceFolder}/myesp32.cfg"
    ],
    "idf.launchMonitorOnDebugSession": true,
    "editor.multiCursorModifier": "alt",
    "files.associations": {
        "*.js": "javascriptreact",
        "*.S": "asm-intel-x86-generic",
        "*.s": "asm",
        "*.h": "cpp",
        "array": "c",
        "*.tcc": "c",
        "deque": "c",
        "list": "c",
        "string": "c",
        "unordered_map": "c",
        "unordered_set": "c",
        "vector": "c",
        "string_view": "c",
        "memory": "c",
        "random": "c",
        "initializer_list": "c",
        "valarray": "c",
        "*.ipp": "c",
        "ranges": "c",
        "fstream": "cpp",
        "ostream": "cpp",
        "sstream": "cpp",
        "streambuf": "cpp",
        "*.inc": "cpp"
    },
    "idf.port": "/dev/cu.usbserial-0001"
}